{
  "name": "bedelightful/task-scheduler",
  "type": "library",
  "license": "Apache License 2.0",
  "keywords": [
    "php",
    "hyperf"
  ],
  "description": "",
  "autoload": {
    "psr-4": {
      "BeDelightful\\TaskScheduler\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BeDelightful\\TaskScheduler\\Test\\": "tests"
    }
  },
  "require": {
    "php": ">=8.1",
    "dragonmantank/cron-expression": "^3.3",
    "hyperf/config": "^3.0",
    "hyperf/crontab": "^3.0",
    "hyperf/db-connection": "^3.0",
    "hyperf/di": "^3.0",
    "hyperf/snowflake": "^3.0",
    "hyperf/logger": "^3.0",
    "hyperf/redis": "^3.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0",
    "mockery/mockery": "^1.0",
    "phpstan/phpstan": "^1.0",
    "phpunit/phpunit": "^10.0",
    "swoole/ide-helper": "dev-master"
  },
  "suggest": {
    "swow/swow": "Required to create swow components."
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit -c phpunit.xml --colors=always",
    "analyse": "phpstan analyse --memory-limit 1024M -l 0 ./src",
    "cs-fix": "php-cs-fixer fix $1"
  },
  "extra": {
    "hyperf": {
      "config": "Delightful\\TaskScheduler\\ConfigProvider"
    },
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  }
}